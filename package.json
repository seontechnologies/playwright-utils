{
  "name": "@seontechnologies/playwright-utils",
  "version": "0.1.14",
  "description": "A collection of utilities for Playwright.",
  "workspaces": [
    "sample-app"
  ],
  "author": "Murat Ozcan",
  "license": "UNLICENSED",
  "main": "dist/playwright-utils.js",
  "types": "dist/playwright-utils.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./api-request": {
      "types": "./dist/api-request/index.d.ts",
      "require": "./dist/api-request/index.js",
      "import": "./dist/api-request/index.js"
    },
    "./api-request/fixtures": {
      "types": "./dist/api-request/fixtures.d.ts",
      "require": "./dist/api-request/fixtures.js",
      "import": "./dist/api-request/fixtures.js"
    },
    "./intercept-network-call": {
      "types": "./dist/intercept-network-call/index.d.ts",
      "require": "./dist/intercept-network-call/index.js",
      "import": "./dist/intercept-network-call/index.js"
    },
    "./intercept-network-call/fixtures": {
      "types": "./dist/intercept-network-call/fixtures.d.ts",
      "require": "./dist/intercept-network-call/fixtures.js",
      "import": "./dist/intercept-network-call/fixtures.js"
    },
    "./log": {
      "types": "./dist/log/index.d.ts",
      "require": "./dist/log/index.js",
      "import": "./dist/log/index.js"
    },
    "./log/fixtures": {
      "types": "./dist/log/fixtures.d.ts",
      "require": "./dist/log/fixtures.js",
      "import": "./dist/log/fixtures.js"
    },
    "./recurse": {
      "types": "./dist/recurse/index.d.ts",
      "require": "./dist/recurse/index.js",
      "import": "./dist/recurse/index.js"
    },
    "./recurse/fixtures": {
      "types": "./dist/recurse/fixtures.d.ts",
      "require": "./dist/recurse/fixtures.js",
      "import": "./dist/recurse/fixtures.js"
    },
    "./auth-session": {
      "types": "./dist/auth-session/index.d.ts",
      "require": "./dist/auth-session/index.js",
      "import": "./dist/auth-session/index.js"
    },
    "./auth-session/fixtures": {
      "types": "./dist/auth-session/fixtures.d.ts",
      "require": "./dist/auth-session/fixtures.js",
      "import": "./dist/auth-session/fixtures.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "homepage": "https://github.com/seontechnologies/playwright-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seontechnologies/playwright-utils.git"
  },
  "bugs": {
    "url": "https://github.com/seontechnologies/playwright-utils/issues"
  },
  "keywords": [
    "playwright",
    "utils"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start:sample-app": "cd sample-app && npm run start",
    "postinstall": "npm run build",
    "publish:local": "./scripts/publish.sh",
    "build": "rm -rf dist && tsc -p tsconfig-build.json",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "fix:format": "npm run prettier -- --write",
    "validate": "npm-run-all --parallel typecheck lint test fix:format",
    "test": "jest --detectOpenHandles --verbose --silent --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint . --cache-location node_modules/.cache/eslint --fix",
    "test:pw": "TEST_ENV=dev npx playwright test",
    "test:pw-ui": "TEST_ENV=dev npx playwright test --ui"
  },
  "peerDependencies": {
    "@playwright/test": "^1.52.0"
  },
  "devDependencies": {
    "@faker-js/faker": "9.7.0",
    "@playwright/test": "^1.52.0",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.15.2",
    "@types/picomatch": "^4.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "dotenv": "^16.5.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-implicit-dependencies": "1.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.2.6",
    "is-ci": "^4.0.0",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "ts-jest": "29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "deepmerge": "^4.3.1",
    "picomatch": "^4.0.2",
    "uuid": "^11.1.0"
  }
}
